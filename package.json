{
    "name": "vscode-eclipse-pydev-windows-keymap",
    "displayName": "Eclipse/PyDev Windows Keymap",
    "description": "Keymap from Eclipse on Windows with PyDev extensions.",
    "version": "0.0.2",
    "publisher": "fabioz",
    "engines": {
        "vscode": "^1.18.0"
    },
    "icon": "icon.png",
    "license": "MIT",
    "homepage": "https://github.com/fabioz/vscode-eclipse-pydev-windows-keymap",
    "repository": {
        "type": "git",
        "url": "https://github.com/fabioz/vscode-eclipse-pydev-windows-keymap"
    },
    "categories": [
        "Keymaps"
    ],
    "contributes": {
        "keybindings": [
            {
                "key": "shift+alt+up",
                "command": "editor.action.smartSelect.grow",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+shift+l ctrl+shift+l",
                "command": "workbench.action.openGlobalKeybindings"
            },
            {
                "key": "ctrl+k",
                "command": "editor.action.addSelectionToNextFindMatch",
                "when": "editorFocus && editorHasSelection"
            },
            {
                "key": "ctrl+k",
                "command": "editor.action.nextMatchFindAction",
                "when": "editorFocus && !editorHasSelection"
            },
            {
                "key": "ctrl+shift+k",
                "command": "editor.action.addSelectionToPreviousFindMatch"
            },
            {
                "key": "ctrl+d",
                "command": "editor.action.deleteLines",
                "when": "editorTextFocus && !editorReadonly"
            },
            {
                "key": "shift+alt+down",
                "command": "editor.action.smartSelect.shrink",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+2 r",
                "command": "editor.action.changeAll",
                "when": "editorTextFocus && !editorReadonly"
            },
            {
                "key": "ctrl+shift+w",
                "command": "workbench.action.closeAllEditors"
            },
            {
                "key": "ctrl+shift+f4",
                "command": "workbench.action.closeAllEditors"
            },
            {
                "key": "f8",
                "command": "workbench.action.debug.continue",
                "when": "inDebugMode"
            },
            {
                "key": "ctrl+alt+down",
                "command": "editor.action.copyLinesDownAction",
                "when": "editorTextFocus && !editorReadonly"
            },
            {
                "key": "ctrl+alt+up",
                "command": "editor.action.copyLinesUpAction",
                "when": "editorTextFocus && !editorReadonly"
            },
            {
                "key": "ctrl+3",
                "command": "workbench.action.showCommands"
            },
            {
                "key": "ctrl+shift+b",
                "command": "editor.debug.action.toggleBreakpoint",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+shift+g",
                "command": "editor.action.referenceSearch.trigger",
                "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
            },
            {
                "key": "ctrl+h",
                "command": "workbench.action.findInFiles",
                "when": "!editorFocus && !searchInputBoxFocus"
            },
            {
                "key": "ctrl+h",
                "command": "workbench.action.findInFiles",
                "when": "editorFocus"
            },
            {
                "key": "ctrl+shift+p",
                "command": "editor.action.jumpToBracket",
                "when": "editorTextFocus"
            },
            {
                "key": "f3",
                "command": "editor.action.goToDeclaration",
                "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
            },
            {
                "key": "f3",
                "command": "C_Cpp.GoToDeclaration",
                "when": "editorTextFocus && editorLangId == 'c'"
            },
            {
                "key": "shift+enter",
                "command": "editor.action.insertLineAfter",
                "when": "editorTextFocus && !editorReadonly"
            },
            {
                "key": "ctrl+f6",
                "command": "workbench.action.openNextRecentlyUsedEditorInGroup"
            },
            {
                "key": "ctrl+f6",
                "command": "workbench.action.quickOpenNavigateNextInEditorPicker",
                "when": "inEditorsPicker && inQuickOpen"
            },
            {
                "key": "ctrl+shift+f6",
                "command": "workbench.action.openPreviousRecentlyUsedEditorInGroup"
            },
            {
                "key": "alt+f3",
                "command": "editor.action.previewDeclaration",
                "when": "editorHasDefinitionProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
            },
            {
                "key": "alt+f3",
                "command": "C_Cpp.PeekDeclaration",
                "when": "editorTextFocus && editorLangId == 'c'"
            },
            {
                "key": "ctrl+1",
                "command": "editor.action.quickFix",
                "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
            },
            {
                "key": "ctrl+.",
                "command": "editor.action.marker.next",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+shift+.",
                "command": "editor.action.marker.prev",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+f7",
                "command": "workbench.action.quickOpenView"
            },
            {
                "key": "ctrl+f7",
                "command": "workbench.action.quickOpenNavigateNextInViewPicker",
                "when": "inQuickOpen && inViewsPicker"
            },
            {
                "key": "ctrl+shift+f7",
                "command": "workbench.action.quickOpenNavigatePreviousInViewPicker",
                "when": "inQuickOpen && inViewsPicker"
            },
            {
                "key": "shift+alt+r",
                "command": "editor.action.rename",
                "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
            },
            {
                "key": "ctrl+5",
                "command": "editor.action.removeCommentLine",
                "when": "editorTextFocus && !editorReadonly"
            },
            {
                "key": "ctrl+shift+f",
                "command": "editor.action.formatSelection",
                "when": "editorTextFocus && editorHasSelection"
            },
            {
                "key": "ctrl+shift+f",
                "command": "editor.action.formatDocument",
                "when": "editorTextFocus && !editorHasSelection"
            },
            {
                "key": "ctrl+shift+s",
                "command": "workbench.action.files.saveAll"
            },
            {
                "key": "ctrl+alt+s",
                "command": "workbench.action.files.saveAs"
            },
            {
                "key": "ctrl+e",
                "command": "workbench.action.showAllEditors"
            },
            {
                "key": "shift+alt+w",
                "command": "workbench.view.explorer"
            },
            {
                "key": "shift+f9",
                "command": "workbench.action.debug.start",
                "when": "!inDebugMode"
            },
            {
                "key": "f9",
                "command": "workbench.action.debug.run",
                "when": "!inDebugMode"
            },
            {
                "key": "F11",
                "command": "workbench.action.debug.start",
                "when": "!inDebugMode"
            },
            {
                "key": "ctrl+F11",
                "command": "workbench.action.debug.run",
                "when": "!inDebugMode"
            },
            {
                "key": "f5",
                "command": "workbench.action.debug.stepInto",
                "when": "inDebugMode"
            },
            {
                "key": "f6",
                "command": "workbench.action.debug.stepOver",
                "when": "inDebugMode"
            },
            {
                "key": "f7",
                "command": "workbench.action.debug.stepOut",
                "when": "inDebugMode"
            },
            {
                "key": "ctrl+f2",
                "command": "workbench.action.debug.stop",
                "when": "inDebugMode"
            },
            {
                "key": "ctrl+alt+f9",
                "command": "workbench.action.debug.stop",
                "when": "inDebugMode"
            },
            {
                "key": "ctrl+m",
                "command": "workbench.action.toggleZenMode"
            },
            {
                "key": "f12",
                "command": "workbench.action.focusActiveEditorGroup"
            },
            {
                "key": "ctrl+shift+t",
                "command": "workbench.action.showAllSymbols"
            },
            {
                "key": "ctrl+o",
                "command": "workbench.action.gotoSymbol"
            },
            {
                "key": "alt+f11",
                "command": "workbench.action.toggleFullScreen"
            },
            {
                "key": "ctrl+shift+r",
                "command": "workbench.action.quickOpen"
            },
            {
                "key": "ctrl+l",
                "command": "workbench.action.gotoLine"
            },
            {
                "key": "ctrl+shift+delete",
                "command": "deleteAllRight",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+alt+o",
                "command": "workbench.action.files.openFile"
            },
            {
                "key": "ctrl+{",
                "command": "workbench.action.splitEditor"
            },
            {
                "key": "ctrl+alt+j",
                "command": "editor.action.joinLines",
                "when": "editorTextFocus"
            },
            {
                "key": "alt+e",
                "command": "editor.action.startFindReplaceAction",
                "when": "findWidgetVisible"
            },
            {
                "key": "alt+r",
                "command": "editor.action.startFindReplaceAction",
                "when": "findWidgetVisible"
            },
            {
                "key": "alt+n",
                "command": "editor.action.nextMatchFindAction",
                "when": "findWidgetVisible"
            },
            {
                "key": "alt+d",
                "command": "editor.action.replaceOne",
                "when": "findWidgetVisible"
            },
            {
                "key": "alt+a",
                "command": "editor.action.replaceAll",
                "when": "findWidgetVisible"
            }
        ]
    }
}